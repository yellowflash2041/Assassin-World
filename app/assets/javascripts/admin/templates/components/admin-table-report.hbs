{{#if model.sortedData}}
  <table class="table report {{model.type}}">
    <tr>
      <th>{{model.xaxis}}</th>
      <th>{{model.yaxis}}</th>
    </tr>

    {{#each model.sortedData as |row|}}
      <tr>
        <td class="x-value">{{row.x}}</td>
        <td>
          {{row.y}}
        </td>
      </tr>
    {{/each}}
  </table>
{{/if}}
