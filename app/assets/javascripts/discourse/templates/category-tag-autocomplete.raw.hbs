<div class='autocomplete'>
  <ul>
    {{#each option in options}}
      <li>
        {{#if option.model}}
          <a href>{{category-link option.model allowUncategorized="true" link="false"}}</a>
        {{else}}
          <a href>{{fa-icon 'tag'}}{{option.text}} x {{option.count}}</a>
        {{/if}}
      </li>
    {{/each}}
  </ul>
</div>
