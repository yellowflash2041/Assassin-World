<div class="names">
  <span class="username">
    {{#if user}}
      <UserLink @user={{user}}>{{user.username}}</UserLink>
      {{#if user.silenced}}
        {{d-icon "ban" title="user.silenced_tooltip"}}
      {{/if}}
    {{else}}
      {{i18n "review.deleted_user"}}
    {{/if}}
  </span>
  <PluginOutlet @name="after-reviewable-post-user" @connectorTagName="div" @args={{hash user=user}} />
</div>
